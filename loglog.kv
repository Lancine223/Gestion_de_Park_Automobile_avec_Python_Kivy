<FirstWindow>:
    name: "first"
    Screen:
        MDSmartTile:
            hint_animation:True

            box_color: 1, 1, 1, .2
            source: "LLL.jpg"
        MDTopAppBar:

            title:"Bienvénue Sur Ma Page Login!"
            font_name: "Spooky Monster.otf"
            theme_text_color: "Custom"
            text_color: "blue"
            halign: "center"
            elevation: 4
            pos_hint: {"top": 1}
            md_bg_color: 0,1,1,1
            specific_text_color: 0, 0, 0, 1

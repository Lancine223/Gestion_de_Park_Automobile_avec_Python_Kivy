                [['menu', lambda x: nav_drawer.set_state("open")]]


        MDCard:
            size_hint: None, None
            size: 700, 500
            pos_hint: {"center_x":0.5,"center_y": 0.6}
            elevaton: 18
            padding: 30
            spacing: 40
            hint_animation:True
            
            orientation: 'vertical'
            shadow_radius: 24
            shadow_softness: 56

            md_bg_color : 0, 0, 0, 0
            shadow_softness_size: 3.5











            MDGridLayout:
                cols: 2
                #orientation: "vertical"
                size_hint: None, None
                pos_hint: {"center_x":0.3,"center_y": 0.8}
                elevaton: 14
                padding: 25
                spacing: 0

                MDTextField:
                    id: user_input
                    color_mode: "custom"
                    text_color: "blue"
                    halign: "center"
                    required: True
                    valign: "center"
                    foreground_color: 1, 0, 1, 1
                    line_color_focus: 1, 0, 1, 1
                    hint_text: "Nom d'utilisateur"
                    hint_text_color_normal:"blue"
                    text_color_normal: "blue"

                    size_hint_x: None
                    width: 300

                    mode: "round"
                    font_name: "Poppins-Bold.ttf"
                    font_size: 20
                    pos_hint: {"center_x": 0.4, "center_y": 0.5}

                MDIconButton:
                    icon: "account"
                    hint_size: None
                    icon_size: "30sp"
                    pos: password_input.width - self.width + dp(8), 0


                    theme_icon_color: "Custom"
                    icon_color: "red"

                MDTextField:
                    id: password_input
                    font_name: "Poppins-Bold.ttf"
                    hint_text: "Mot de Pass"

                    required: True
                    color_mode: "custom"
                    text_color: "blue"
                    halign: "center"
                    text_color_normal: "blue"
                    required: True
                    valign: "center"
                    foreground_color: 1, 0, 1, 1
                    line_color_focus: 1, 0, 1, 1
                    hint_text: "Mot de pass"
                    hint_text_color_normal:"blue"




                    disable: False
                    text_color: "blue"


                    size_hint_x: None
                    width: 300
                    mode : "round"

                    font_size: 18
                    pos_hint: {"center_x": 0.5}
                MDIconButton:
                    id: icon
                    pos: password_input.width - self.width + dp(8), 0
                    icon: "key-variant"
                    hint_size: None
                    icon_size: "30sp"
                    theme_icon_color: "Custom"
                    icon_color: "red"


                    on_release:
                        root.lock_word()


            MDGridLayout:
                cols: 3
                size_hint: None, None
                pos_hint: {"center_x":0.3,"center_y": 0.8}
                elevaton: 14
                padding: 25
                spacing: 25

                MDFlatButton:

                    md_bg_color: 0,0,0,0
                    text_color: "white"

                    hint_animation:True
                    font_size: 12


                MDRoundFlatButton:
                    background_color: "white"
                    text: "SE CONNECTER"
                    text_color: "white"
                    selected_color_background: "orange"
                    text_color_active: "yellow"
                    hint_animation:True
                    font_size: 12

                    font_name: "Poppins-Bold.ttf"

                    md_bg_color: "blue"


                    on_press:
                        root.validate_user(user_input, password_input)


                MDFlatButton:
                    md_bg_color: 0,0,0,0


                    hint_animation:True
                    font_size: 12







        MDFlatButton:
            text: "Créer un compte"
            theme_text_color: "Custom"
            text_color: "blue"
            text_color_active: "yellow"
            hint_animation:True
            font_size: 12
            font_name: "Poppins-Bold.ttf"
            md_bg_color: 0,0,0,0
            pos_hint: {"center_x": 0.4,"center_y": 0.2}
            on_press:

                root.refuser_useru()



        MDFlatButton:
            text: "Mot de pass oublié"
            theme_text_color: "Custom"
            text_color: "red"

            text_color_active: "yellow"
            hint_animation:True
            font_size: 12
            pos_hint: {"center_x": 0.6,"center_y": 0.2 }
            font_name: "Poppins-Bold.ttf"
            md_bg_color: 0,0,0,0
            on_press:
                root.manager.current= "pass_oublier"






        MDNavigationDrawer:
            id: nav_drawer
            orientation: "vertical"
            md_bg_color : 0, 1, 1, 1
            MDCard:
                radius: 36
                md_bg_color : 0, 1, 1, 1
                pos_hint: {"center_x": .5, "center_y": .5}
                FitImage:
                    source: 'lastlogin_log.jpg'
                    pos_hint: {"top": 1}
                    size_hint: (.5, .5)
                    radius: 36, 36, 0, 0





            MDBoxLayout:
                orientation: "vertical"
                MDFillRoundFlatIconButton:
                    text: "Créer un compte"
                    text_color: "blue"
                    font_name: "Poppins-Bold.ttf"
                    md_bg_color: 0,0,0,0

                    icon: "add-user.png"

                    icon_size: "55sp"
                    pos_hint: {"center_x": .5, "center_y": .5}
                    on_press: root.refuser_useru()
                    #on_press: root.manager.current= "creer_user"

                    icon_left: "plus"










